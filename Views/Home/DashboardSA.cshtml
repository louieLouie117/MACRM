<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SA Dashboard</title>
</head>
<script>

    const GetAPIDataHandler = (e) => {
        alert("button was click");


        $.ajax({
            type: "Get",
            url: '@Url.Action("APIDataMethod", "Home")',
            success: APIDataMethodSuccess,
        });
    }

    const APIDataMethodSuccess = (result) => {
        console.log("call back result", result);
        @* APIResult(result); *@
        return
    }

</script>

<body>
    <h1>SA Dashboard</h1>
    <p>This is going to be where the SA Dashboard lived.</p>
    <button onclick="GetAPIDataHandler(event)">Get API Data</button>

</body>

</html>