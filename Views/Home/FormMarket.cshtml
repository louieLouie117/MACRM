<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Maket Form</title>

    <script async>
        $.get("/AllMarketList", data => {
            console.log("getting jobs Data:", data)
            RenderMarketList(data);
            return

        });

        const RenderMarketList = (data) => {

            $('#AllMarketsList').empty();
            console.log("Market Data", data.result)
            $.each(data.result, function (i, v) {
                console.log(i, v.name);

                $(`
            <li id="${v.marketId}">

                    <p id="${v.marketId}"> ${v.markerCode}</p>
                    <p id="${v.marketId}"> ${v.location}</p>

            </li>

            `).prependTo("#AllMarketsList");

            })

        }
    </script>
</head>

<body>

    <div class="FormMarket">

        <main>
            <h2>Markets</h2>
            <form action="">
                <input id="MarketLocation" type="text" placeholder="Market Location">
                <input id="MarketCode" type="text" placeholder="Market Code">
                <button onclick="CommandCenterFormHandler(event)">+ Market</button>
            </form>
        </main>
        <aside>
            <h2>This is a list of all markets</h2>
            <ul id="AllMarketsList"></ul>
        </aside>

    </div>

</body>

</html>