<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Pre-Screen</title>
</head>

<body>
    <div id="PreScreenContainer" class="PreScreen-Container">
        <main>
            <div class="PreScreenLayout">
                <section>
                    <h2 onclick="PreScreenHandler(event)">&#60; Job Details</h2>
                    <form action="">
                        <h3>Service</h3>
                        <input id="ServiceJobNumberPSF" type="text">
                        <input id="SamsungJobNumberPSF" type="text">
                        <input id="InWarrantyPSF" type="text">

                        <h3>Customer Info</h3>
                        <input id="NamePSF" type="text">
                        <input id="AddressPSF" type="text">
                        <input id="CityPSF" type="text">
                        <input id="StatePSF" type="text">
                        <input id="ZipcodePSF" type="text">
                        <input id="PhoneNumberPSF" type="text">
                        <input id="EmailPSF" type="text">

                        <h3>Product</h3>
                        <input id="ProductLinePSF" type="text">
                        <input id="ModelNumberPSF" type="text">
                        <input id="SerialNumberPSF" type="text">
                        @* <div class="hidden"> *@
                        <h3>User Assigned</h3>
                        <input id="TwoManJobPSF" type="text">
                        <input id="TechNamePSF" type="text">
                        <input id="SANamePSF" type="text">
                        <input id="SecondPersonNamePSF" type="text">

                        <h3>Appoinment Info</h3>
                        <input id="AppointmentDayPSF" type="text">
                        <input id="AppointmentTimePSF" type="text">
                        <input id="AppointmentWindowPSF" type="text">
                        <input id="SpecialInstructionsPSF" type="text">
                        <input id="NumberOfContactMadePSF" type="text">
                        <input id="NumberOfTimesReschedualPSF" type="text">

                        <h3>Market Info</h3>
                        <input id="MarketPSF" type="text">
                        <input id="MarketCodePSF" type="text">

                        <h3>Market Info</h3>
                        <input id="ServiceStatusPSF" type="text">
                        <input id="JobStatusPSF" type="text">
                        <input id="JobStatusColorPSF" type="text">
                        <input id="ReworkPSF" type="text">
                        <input id="ForceBookPSF" type="text">





                        @* </div> *@




                    </form>

                </section>

                <section>
                    <h2>Click Options</h2>
                </section>

                <section>
                    <h2>Pre-screen</h2>
                </section>
            </div>

        </main>
    </div>

</body>

</html>