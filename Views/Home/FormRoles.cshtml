<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Roles</title>

    <script async>

        const RenderRole = (data) => {

            $('#AllRoleList').empty();
            console.log("Role Data", data.result)
            $.each(data.result, function (i, v) {
                console.log(i, v.name);

                $(`
            <li id="${v.roleId}">

                    <p id="${v.roleId}"> ${v.title}</p>

            </li>

            `).prependTo("#AllRoleList");

            })

        }
    </script>
</head>



<body>
    <div class="FormRoles-Container">
        <h1>Roles</h1>

        <main>
            <form action="">
                <input type="text" placeholder="Role" id="UserRole">
                <button onclick="CommandCenterFormHandler(event)">+ Role</button>
            </form>
        </main>
        <aside>
            <ul id="AllRoleList">

            </ul>
        </aside>

    </div>
</body>

</html>